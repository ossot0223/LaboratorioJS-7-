


  let numeros = [5,6,3,4,0,-6,-6,-9,0,0];

let positivos =0;
let negativos =0;
let ceros =0;
numeros.forEach(
 numero => {
  if(numero>0) positivos++;
  if(numero==0) ceros++;
  if(numero<0) negativos++;
 }
)
console.log('positivos '+positivos);
console.log('negativos '+negativos);
console.log('ceros '+ceros);
